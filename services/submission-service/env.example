# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/photo_classification_db

# JWT Configuration (same secret key as user service)
SECRET_KEY=your-secret-key-here-change-in-production-min-32-characters
ALGORITHM=HS256

# Classification Service
CLASSIFICATION_SERVICE_URL=http://localhost:8003

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg

# Application Settings
DEBUG=False
LOG_LEVEL=INFO

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

